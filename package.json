{
  "name": "vue-blocklink",
  "version": "0.2.108",
  "description": "Vue support for the Blockchain Link browser extension",
  "main": "src/index.js",
  "scripts": {
    "dev": "parcel serve watch src/entry/index.html",
    "build": "parcel build src/entry/index.html --no-cache --no-source-maps --detailed-report",
    "test": "jest",
    "lint": "eslint './**/*.{js,vue}'",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "repository": "git@github.com:tokenchain/vue-blocklink.git",
  "keywords": [
    "vue",
    "vue2",
    "tron",
    "trx",
    "tronlink",
    "ethlink",
    "metamask"
  ],
  "author": "HeskemoKondax",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tokenchain/vue-blocklink/issues"
  },
  "homepage": "https://github.com/tokenchain/vue-blocklink.git",
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "reporters": [
      "default",
      "jest-junit"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@vue/component-compiler-utils": "^3.2.0",
    "babel-jest": "^24.9.0",
    "babel-preset-vue": "^2.0.2",
    "coveralls": "^3.1.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.3",
    "jest": "^24.9.0",
    "jest-junit": "^6.4.0",
    "node-sass": "^4.14.1",
    "parcel-bundler": "^1.12.4",
    "postcss-modules": "^3.2.2",
    "tronweb": "^2.10.2",
    "web3": "^1.3.6",
    "vue": "^2.5.17",
    "lodash": "^4.17.15",
    "vue-hot-reload-api": "^2.3.4",
    "vue-template-compiler": "^2.6.12"
  },
  "dependencies": {
    "bignumber.js": "^9.0.1",
    "elliptic": "^6.5.3",
    "ethereumjs-account": "^3.0.0",
    "ethereumjs-blockstream": "^7.0.0",
    "ethereumjs-vm": "^4.2.0",
    "ethers": "^5.0.17",
    "eventemitter3": "^4.0.7",
    "injectpromise": "^1.0.0",
    "js-sha3": "^0.7.0",
    "semver": "^7.3.2",
    "uuid": "^3.4.0",
    "valid-url": "^1.0.9",
    "validator": "^13.1.17"
  }
}
