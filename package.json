{
  "name": "vue-blocklink",
  "version": "0.1.252",
  "description": "Vue support for the Blockchain Link browser extension",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "scripts": {
    "dev": "vue-cli-service serve --mode development",
    "dev2": "parcel watch test/entry/index.html --no-source-maps",
    "nuxtbuild-serv": "vue-cli-service serve --mode production",
    "nuxtbuild-demo": "vue-cli-service build --name index ./src/main.js",
    "nuxtbuild-lib": "vue-cli-service build --target lib --name index ./src/index.js",
    "parcelbuild": "parcel test/entry/index.html --no-cache --no-source-maps --detailed-report",
    "test": "jest",
    "lint": "eslint './**/*.{js,vue}'",
    "check": "tsc --noEmit",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "engines": {
    "node": "^12.20.0 || ^14.15.0 || ^16.10.0",
    "yarn": ">=1.21.1 <2",
    "npm": "Please use yarn instead of NPM to install dependencies"
  },
  "repository": "git@github.com:tokenchain/vue-blocklink.git",
  "keywords": [
    "vue",
    "vue2",
    "tron",
    "trx",
    "tronlink",
    "ethlink",
    "metamask"
  ],
  "source": "src/index.js",
  "module": "dist/index.js",
  "author": "HeskemoKondax",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tokenchain/vue-blocklink/issues"
  },
  "homepage": "https://github.com/tokenchain/vue-blocklink.git",
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "reporters": [
      "default",
      "jest-junit"
    ]
  },
  "resolutions": {
    "@types/node": "12.12.37"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@vue/component-compiler-utils": "^3.2.0",
    "typescript": "4.4.2",
    "@typescript-eslint/types": "4.31.0",
    "@typescript-eslint/parser": "4.31.0",
    "@typescript-eslint/eslint-plugin": "4.31.0",
    "babel-jest": "^24.9.0",
    "babel-preset-vue": "^2.0.2",
    "coveralls": "^3.1.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.3",
    "jest": "^24.9.0",
    "jest-junit": "^6.4.0",
    "node-sass": "^4.14.1",
    "@vue/cli-service": "4.5.13",
    "postcss-modules": "^3.2.2",
    "web3": "^1.6.0",
    "vue": "^2.5.17",
    "lodash": "^4.17.15",
    "vue-hot-reload-api": "^2.3.4",
    "vue-template-compiler": "^2.6.12"
  },
  "dependencies": {
    "@metamask/detect-provider": "^1.2.0",
    "@metamask/onboarding": "^1.0.1",
    "bignumber.js": "^9.0.1",
    "elliptic": "^6.5.3",
    "eth-sig-util": "^3.0.1",
    "ethereumjs-account": "^3.0.0",
    "ethereumjs-blockstream": "^7.0.0",
    "ethereumjs-util": "^7.1.0",
    "@ethereumjs/vm": "^5.5.2",
    "@ethereumjs/tx": "^3.3.0",
    "ethers": "^5.0.17",
    "eventemitter3": "^4.0.7",
    "injectpromise": "^1.0.0",
    "isomorphic-fetch": "^3.0.0",
    "js-sha3": "^0.7.0",
    "semver": "^7.3.2",
    "uuid": "^3.4.0",
    "valid-url": "^1.0.9",
    "validator": "^13.1.17"
  }
}
